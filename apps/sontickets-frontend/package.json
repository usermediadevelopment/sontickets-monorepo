  "scripts": {
    "dev": "vite --mode development",
    "prod": "vite --mode production",
    "build:prod": "tsc && vite build",
    "build:dev": "tsc && vite build --mode development",
    "preview": "vite preview",
    "deploy:prod": "export NODE_OPTIONS=--max_old_space_size=4096 && npm run build:prod &&  firebase use default && firebase deploy",
    "deploy:dev": "export NODE_OPTIONS=--max_old_space_size=4096 && npm run build:dev && firebase use dev && firebase deploy",
